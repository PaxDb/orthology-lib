{
    "@context": "http://api.pax-db.org/orthology-context.jsonld",
    "@id": "/protein/<%= protein_id %>/ortholog_groups/<%= group.taxonomicLevel %>/<%= tissue %>",
    "@type": "OrhologGroup",
    "taxonomicLevel": "<%= group.taxonomicLevel %>",
    "tissues" : [
        <% for (var numTissues = group.tissues.length, t = 0; t < numTissues; t++) {
            var tissue = group.tissues[t];    %>
            {"<%= tissue %>" : "/protein/<%= protein_id %>/ortholog_groups/<%= group.taxonomicLevel %>/<%= tissue %>"}
        <% if (t < numTissues- 1 ) { %> , <% } %>
        <% } %>
        ],
    "members" : [
        <% for (var numMembers = group.members.length, m = 0; m < numMembers; m++) {
            var member = group.members[m];    %>

            {   "@id" : "proteins.api.pax-db.org/protein/<%= member.id %>",
                "@type": "Protein",
                "abundance":{
                        "value": "<%= member.abundance.value %>",
                        "unit": "UO:0000169",
                        "tissue": "<%= tissue %>",
                        "position": "<%= member.abundance.position %>",
                        "_rank": "<%= member.abundance._rank %>"
                }
            }
        <% if (m < numMembers- 1 ) { %> , <% } %>
        <% } %>
    ]

}